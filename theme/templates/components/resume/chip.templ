package resume

templ Chip(text, variant string) {
	<span>
		switch (variant) {
		case "filled":
			<strong class="font-normal text-neutral-content bg-neutral/80 px-1 rounded-sm mr-1">
				{ text }
			</strong>
		case "css":
			<strong class="font-normal text-neutral-content bg-neutral/80 px-1 rounded-sm mr-1">
				{ text }
			</strong>
		default:
			<strong class="font-normal text-neutral-content bg-neutral/80 px-1 rounded-sm mr-1">
				{ text }
			</strong>
		}
	</span>
}
